<%=form_for(:session, url: login_path) do |f|%>

  	<div class="field">
    	<%= f.label :email %><br>
    	<%= f.email_field :email %><br>
  	</div>
  	<div class="field">
    	<%= f.label :password %><br>
    	<%= f.password_field :password %><br>
  	</div>
    <hr>
    <div class="actions">
      <%= f.submit "Sign In"%>
    </div>
<% end %>